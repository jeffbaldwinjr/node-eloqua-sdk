{
  "verbose": true,
  "git": {
    "commit": true,
    "commitMessage": "Release ${version}",
    "commitArgs": "",
    "tag": true,
    "tagName": "${version}",
    "tagAnnotation": "Release ${version}",
    "tagArgs": "",
    "push": true,
    "pushArgs": "",
    "pushRepo": "origin",
    "requireUpstream": false
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": true
  },
  "dist": {
    "repo": "https://github.com/jeffbaldwinjr/node-eloqua-sdk.git#gh-pages",
    "scripts": {
      "beforeStage": "npm run docs-build && npm run test-local && cp -a /docs/. /.stage/ && cp -a /coverage/. /.stage/coverage/ && rm -rf docs && rm -rf coverage"
    }
  }
}
